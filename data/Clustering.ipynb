    "productos[\"precio\"] = productos[\"precio\"].str.replace(\"€\", \"\").str.replace(\".\", \"\").str.replace(\",\", \".\").astype(float)\n",
    "productos[\"rating\"] = productos[\"rating\"].str.replace(\"/5\", \"\").str.replace(\",\", \".\").replace(\"0\", None).astype(float)\n",
    "import re\n",
    "\n",
    "def extraer_opiniones(x):\n",
    "    num = re.findall(r'\\d+', x.replace('.', ''))\n",
    "    return int(num[0]) if num else 0\n",
    "\n",
    "productos[\"opiniones\"] = productos[\"opiniones\"].apply(extraer_opiniones)\n"
   ]
