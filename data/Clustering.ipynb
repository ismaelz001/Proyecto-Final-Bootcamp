    "# Cargar el CSV\n",
    "caracteristicas = pd.read_csv(\"caracteristicas_portatiles_actualizado.csv\")\n",
    "\n",
    "# Limpiar y convertir Processor Speed (GHz)\n",
    "def limpiar_ghz(valor):\n",
    "    if pd.isnull(valor):\n",
    "        return None\n",
    "    valor = valor.lower().replace('ghz', '').replace(',', '.').replace(' ', '')\n",
    "    try:\n",
    "        return float(valor)\n",
    "    except:\n",
    "        return None\n",
    "\n",
    "caracteristicas[\"Processor Speed\"] = caracteristicas[\"Processor Speed\"].apply(limpiar_ghz)\n",
    "\n",
    "# Limpiar y convertir Processor Cores (n√∫cleos)\n",
    "caracteristicas[\"Processor Cores\"] = pd.to_numeric(caracteristicas[\"Processor Cores\"], errors='coerce')\n",
    "\n",
    "# Limpiar y convertir RAM Gbs\n",
    "def limpiar_ram(valor):\n",
    "    if pd.isnull(valor):\n",
    "        return None\n",
    "    valor = valor.lower().replace('gb', '').replace(' ', '')\n",
    "    try:\n",
    "        return int(valor)\n",
    "    except:\n",
    "        return None\n",
    "\n",
    "caracteristicas[\"RAM Gbs\"] = caracteristicas[\"RAM Gbs\"].apply(limpiar_ram)\n",
    "\n",
    "# Limpiar y convertir Storage Gbs\n",
    "def limpiar_storage(valor):\n",
    "    if pd.isnull(valor):\n",
    "        return None\n",
    "    valor = valor.lower().replace('gb', '').replace(' ', '')\n",
    "    try:\n",
    "        return int(valor)\n",
    "    except:\n",
    "        return None\n",
    "\n",
    "caracteristicas[\"Storage Gbs\"] = caracteristicas[\"Storage Gbs\"].apply(limpiar_storage)\n",
    "\n"
