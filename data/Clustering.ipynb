    "\n",
    "# Definir las marcas\n",
    "marcas = [\"Apple\", \"HP\", \"Lenovo\", \"ASUS\", \"MSI\", \"Acer\", \"PcCom\", \"Alurin\"]\n",
    "\n",
    "# Función para detectar la marca desde el nombre del producto\n",
    "def detectar_marca(texto):\n",
    "    for marca in marcas:\n",
    "        if marca.lower() in texto.lower():\n",
    "            return marca\n",
    "    return \"Desconocida\"\n",
    "\n",
    "# Añadir columna 'marca' al dataframe productos\n",
    "productos[\"marca\"] = productos[\"nombre\"].apply(detectar_marca)\n",
    "\n",
    "# Unir productos con características\n",
    "df = productos.merge(caracteristicas, on=\"producto_id\", how=\"left\")\n",
    "\n",
    "# Unir con categorías (si quieres quedarte con el nombre de la categoría)\n",
    "df = df.merge(categorias[[\"id\", \"nombre\"]], left_on=\"categoria_id\",right_on=\"id\", how=\"left\")\n",
    "df = df.rename(columns={\"nombre\": \"categoria\"})\n",
    "\n"
